{
    "name": "mediatis/one-trust-utility",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2 || ^8.0",
        "typo3/cms-core": "^9.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "Mediatis\\OneTrustUtility\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediatis\\OneTrustUtility\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/one-trust-utility": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ],
        "test": "phpunit --testdox --colors=always Tests/"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "one_trust_utility"
        }
    }
}